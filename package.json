{
  "name": "space-x",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run spaceX:serve-ssr",
    "serve:ssr": "node dist/spaceX/server/main.js",
    "build:ssr": "ng build --prod && ng run spaceX:server:production",
    "app:watch": "rimraf dist && npm-run-all -p sXwatch:*",
    "sXwatch:client": "ng build --build-optimizer=false --aot=false  --extract-css=false --source-map=true --watch",
    "sXwatch:webpack": "node ./node_modules/npm-delay 10000 && webpack --config webpack.config.js --watch",
    "sXwatch:nodemon": "node ./node_modules/npm-delay 45000 && nodemon  dist/server.js",
    "prerender": "ng run spaceX:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.6",
    "@angular/common": "~10.1.6",
    "@angular/compiler": "~10.1.6",
    "@angular/core": "~10.1.6",
    "@angular/forms": "~10.1.6",
    "@angular/platform-browser": "~10.1.6",
    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/platform-server": "~10.1.6",
    "@angular/router": "~10.1.6",
    "@nguniversal/express-engine": "^10.1.0",
    "bootstrap": "^4.5.3",
    "express": "^4.15.2",
    "jquery": "^3.5.1",
    "ngx-spinner": "^10.0.1",
    "npm-delay": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "run-p": "0.0.0",
    "rxjs": "~6.6.0",
    "ts-loader": "^8.0.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "~10.1.7",
    "@angular/compiler-cli": "~10.1.6",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^4.1.0",
    "webpack-shell-plugin": "^0.5.0"
  }
}
